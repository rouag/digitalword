<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<data>

		<template id="report_header_external_custom" inherit_id="report.external_layout_header">
            <xpath expr="//div[@class='header']" position="replace">
                <div class ="header">

				<div class="row">
					<div class="col-xs-6 text-left"
					>
 <img t-if="company.logo" t-att-src="'data:image/png;base64,%s' % company.logo" style="height:180px;width: 180px;" />

					</div>
					<div class="col-xs-6 text-right">

							<strong>
								Digital World
								 <br></br>
							</strong>
					 </div>
				</div>
			    </div>
            </xpath>

		</template>
	</data>
</odoo>
